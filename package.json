{
  "name": "vcdb",
  "version": "0.0.1",
  "description": "Minimal, dependencyâ€‘free vector database with pluggable ANN strategies (bruteforce, HNSW, IVF), attribute filtering, and a clean persistence model (index/data split with CRUSH placement). Runs in Node.js, browsers (OPFS), Electron, and Tauri.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "import": "./dist/client/index.js",
      "require": "./dist/client/index.cjs",
      "default": "./dist/client/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js",
      "require": "./dist/cli/index.cjs",
      "default": "./dist/cli/index.js"
    },
    "./http-server": {
      "types": "./dist/http-server/index.d.ts",
      "import": "./dist/http-server/index.js",
      "require": "./dist/http-server/index.cjs",
      "default": "./dist/http-server/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js",
      "require": "./dist/config/index.cjs",
      "default": "./dist/config/index.js"
    },
    "./storage/*": {
      "types": "./dist/storage/*.d.ts",
      "import": "./dist/storage/*.js",
      "require": "./dist/storage/*.cjs",
      "default": "./dist/storage/*.js"
    },
    "./presets/config/*": {
      "types": "./dist/presets/config/*.d.ts",
      "import": "./dist/presets/config/*.js",
      "require": "./dist/presets/config/*.cjs",
      "default": "./dist/presets/config/*.js"
    }
  },
  "packageManager": "bun@1.2.20",
  "sideEffects": false,
  "bin": {
    "vcdb": "bin/vcdb.cjs"
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "scripts": {
    "build": "vite build",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "cli": "bun run src/cli/main.tsx",
    "doc:compile": "node scripts/doc-compile.mjs",
    "check:node-deps": "node scripts/check-node-deps.mjs",
    "seed": "bun run src/cli/tools/seed.ts"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "ajv": "^8.17.1",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^54.1.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.0",
    "hono": "^4.9.4",
    "ink": "^6.2.2",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
